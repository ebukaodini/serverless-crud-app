{
  "name": "serverless-crud-app",
  "version": "1.0.0",
  "description": "serverless crud app",
  "scripts": {
    "build": "tsc",
    "start": "node ./bin/www",
    "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./bin/www\"",
    "start:sls": "serverless offline",
    "pretest": "tsc",
    "prettier": "prettier --write --ignore-unknown ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "password-hash": "^1.2.2",
    "serverless-http": "^3.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/mysql": "^2.15.22",
    "@types/password-hash": "^1.2.22",
    "concurrently": "^8.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "serverless-offline": "^13.2.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^4.9.5"
  }
}
